public class Solution {
    // DO NOT MODIFY THE LIST. IT IS READ ONLY
    public int lengthOfLastWord(final String A) {
        
        int ans=0;
        for(int i=0;i<A.length();i++){
            if(A.charAt(i)==' ')
            ans=0;
            else
            ans++;
        }
        return ans;
    }
}
